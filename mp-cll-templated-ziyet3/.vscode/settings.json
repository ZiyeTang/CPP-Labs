{
    "clang-format.fallbackStyle": "Google",
    "editor.formatOnSave": true,
    "clang-tidy.lintOnSave": true,
    "clang-tidy.blacklist": [],
    "clang-tidy.compilerArgs": [
        "-std=c++20"
    ],
    "clang-tidy.compilerArgsBefore": [
        "-Iincludes",
        "-Wunused-variable",
        "-Wuninitialized",
        "-Wunnused"
    ],
    "C_Cpp.clang_format_sortIncludes": false,
    "C_Cpp.formatting": "Disabled",
    "C_Cpp.inactiveRegionOpacity": null,
    "files.exclude": {
        "**/.*": true
    },
    "files.associations": {
        "cwchar": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "iterator": "cpp",
        "functional": "cpp",
        "random": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "istream": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp"
    }
}